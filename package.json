{
  "name": "nextjs-fastRPC",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastRPC": "venv/bin/python -m pip install -r requirements.txt && venv/bin/python3 -m uvicorn api.index:app --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastRPC\" \"npm run watch-build-openapi-types\"",
    "watch-gen-client": "npm-watch gen-client",
    "gen-client": "npx openapi-typescript-codegen -i FastRPC/openapi.json -o py_client --name PythonClient --useOptions --exportSchemas true",
    "dev-back-and-types": "concurrently \"npm run fastapi-dev\" \"npm run watch-gen-client\""
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "ajv": "^8.12.0",
    "autoprefixer": "10.4.14",
    "bootstrap": "^5.3.2",
    "concurrently": "^8.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "next": "13.4.4",
    "plotly.js": "^2.27.0",
    "postcss": "8.4.23",
    "prisma": "^5.3.1",
    "react": "18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "18.2.0",
    "react-plotly.js": "^2.6.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "watch": {
    "build-openapi-types": {
      "patterns": [
        "api"
      ],
      "extensions": "json,txt"
    },
    "gen-client": {
      "patterns": [
        "api"
      ],
      "extensions": "json,txt"
    },
    "build": {
      "patterns": [
        "app"
      ],
      "extensions": "js,jsx,html,ts,tsx"
    }
  },
  "devDependencies": {
    "@types/react-plotly.js": "^2.6.2",
    "npm-watch": "^0.11.0",
    "openapi-typescript": "^6.7.0",
    "openapi-typescript-codegen": "^0.25.0"
  }
}
